package app;

import entidades.Fruta;

public class Aplication {
	public static void main(String[] args) {
		Fruta maca = new Fruta("Maçã", true, false, true);
		System.out.println(maca.status());
		maca.comerFruta();
		System.out.println(maca.status());

		Fruta abacate = new Fruta("Abacate", true, true, false);
		System.out.println(abacate.status());
		abacate.comerFruta();
		System.out.println(abacate.status());
	}
}


